{
    "require": {
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.3",
        "psr/container": "^2.0",
        "symfony/mailer": "^6.0",
        "doctrine/dbal": "^3.3",
        "doctrine/orm": "^2.11",
        "symfony/cache": "^6.0",
        "doctrine/migrations": "^3.4",
        "php-di/php-di": "^7.1",
        "slim/twig-view": "^3.4",
        "slim/slim": "4.*",
        "slim/psr7": "^1.8",
        "twig/intl-extra": "^3.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/boost": "^1.8",
        "symfony/maker-bundle": "^1.65"
    }
}
